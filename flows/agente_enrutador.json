        {
            "parameters": {
                "model": "llama3-70b-8192",
                "options": {}
            },
            "id": "groq-lm",
            "name": "Groq Chat Model",
            "type": "@n8n/n8n-nodes-langchain.lmChatGroq",
            "typeVersion": 1,
            "position": [
                400,
                0
            ],
            "credentials": {
                "groqApi": {
                    "id": "nfpxWh3Y5sUHefoj"
                }
            }
        }
    ],
    "connections": {
        "Odoo LiveChat Webhook": {
            "main": [
                [
                    {
                        "node": "Tiene Especialista?",
                        "type": "main",
                        "index": 0
